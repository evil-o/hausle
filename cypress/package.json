{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "prepare": "cypress install",
    "preview": "cd ../app && npm run preview",
    "vite-dev": "cd ../app && npm run dev-test",
    "test:e2e": "DEBUG=start-server-and-test WAIT_ON_TIMEOUT=10000 start-server-and-test vite-dev http-get://127.0.0.1:4200/index.html 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test vite-dev http://0.0.0.0:4200 'cypress open --e2e'",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vue": "~10.6.2",
    "prettier": "3.7.4",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.3"
  }
}